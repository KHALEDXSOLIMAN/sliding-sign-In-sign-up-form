    "htmlmin": "0.0.7",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.5.4",
    "rimraf": "^3.0.2",
    "time-grunt": "^2.0.0",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.9.4",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
